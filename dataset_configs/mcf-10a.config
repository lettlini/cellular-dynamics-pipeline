params {
    drop_first_n = 0
    drop_last_m = 0
    out_dir = "monolayer/juergen/"
    in_dir = "monolayer/juergen/2018-*"
    mum_per_px = 1.08122
    delta_t_minutes = 10
    min_nucleus_area_mumsq = 50
    stardist_probality_threshold = 0.479071
    cell_cutoff_mum = 20
    lag_times_minutes = "30,60,90,120,150,180,210,240"
    minimum_neighbors = 2
    provider = "juergen"
}
process {
    time = "2h"
    memory = "24GB"

    // High CPU processes
    withLabel: high_cpu {
        cpus = 48
    }
    withLabel: low_cpu {
        cpus = 2
    }

    withLabel: short_running {
        time = "30m"
    }

    withLabel: long_running {
        time = "6h"
    }
}
